{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "antd-color-editor": ["src"],
      "antd-color-editor/*": ["src/*"]
    }
  },
  "include": ["src", ".dumi/**/*", ".dumirc.ts", "*.ts"]
}
